<%- include("partials/heading"); %>
        <div class="box error">
            <h3><%= error %> </h3>
        </div>
        <div class="box" id="heading">
            <h1><%= title %> </h1>
        </div>

        <div class="box">
            <form action="/delete" method="POST">
                <% userItems.forEach(function(element){ %>
                    <div class="item">
                        <input type="checkbox" name="checkbox" value="<%= element._id %>" onChange="this.form.submit()">
                        <p><%= element.name %></p>
                    </div>   
                <% });%>
                <input type="hidden" name="listTopic" value=<%= title %>>
            </form>
                        
            <form class="item" method="POST" action="/">
                <input type="text" name="requestedItem" placeholder="Enter item" autocomplete="off">
                <button type="submit" name="type" value=<%= title %> >+</button>
            </form>
        </div>
<%- include("partials/footer") %>
        
